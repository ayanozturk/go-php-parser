{
    "name": "ayan/demo_project",
    "description": "A Symfony web framework project",
    "type": "project",
    "require": {
        "symfony/symfony": "^7.0",
        "symfony/flex": "^2.5",
        "doctrine/dbal": "^3",
        "doctrine/doctrine-bundle": "^2.14",
        "doctrine/doctrine-migrations-bundle": "^3.4",
        "doctrine/orm": "^3.3",
        "symfony/asset": "*",
        "symfony/asset-mapper": "*",
        "symfony/doctrine-messenger": "^7.2",
        "symfony/expression-language": "*",
        "symfony/form": "*",
        "symfony/http-client": "*",
        "symfony/intl": "*",
        "symfony/mailer": "*",
        "symfony/mime": "*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "*",
        "symfony/process": "*",
        "symfony/security-bundle": "*",
        "phpdocumentor/reflection-docblock": "^5.6",
        "phpstan/phpdoc-parser": "^2.1",
        "symfony/property-access": "*",
        "symfony/property-info": "*",
        "symfony/serializer": "*",
        "symfony/stimulus-bundle": "^2.24",
        "symfony/string": "*",
        "symfony/translation": "*",
        "symfony/twig-bundle": "*",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "symfony/ux-turbo": "^2.24",
        "symfony/validator": "*",
        "symfony/web-link": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "symfony/debug-bundle": "*",
        "symfony/stopwatch": "*",
        "symfony/web-profiler-bundle": "*",
        "symfony/maker-bundle": "^1.0",
        "symfony/browser-kit": "*",
        "symfony/css-selector": "*",
        "symfony/phpunit-bridge": "^7.2"
    },
    "scripts": {
        "start": "symfony server:start",
        "test": "phpunit"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "allow-plugins": {
            "symfony/flex": true
        }
    }
}
